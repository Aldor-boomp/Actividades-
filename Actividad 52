#include  <stdio.h>
#include  <stdlib.h>
#include <string.h>
int main (){
char palabra_1[10];
printf("ingresa una palabra:");
scanf("%s", &palabra_1);
char palabra_2[10];
printf("ingresa otra palabra:");
scanf("%s", &palabra_2);
char palabra_3[10];
strcpy (palabra_3, palabra_1);
strcpy (palabra_1, palabra_2);
printf("palabra 1: %s\n", palabra_1);
printf("palabra 2: %s\n", palabra_3);
system("Pause");
return 0;
}