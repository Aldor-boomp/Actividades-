#include  <stdio.h>
#include  <stdlib.h>
int main (){
int are[20];
double sum=0;
long long mul=1;
float prom;
for(int i=0; i<20; i++){
    are[i]= i;
}
for(i=0; i<20; i++){
    sum+=are[i];
    mul*=are[i];
    
}
prom=sum/2;
printf("%d\n",sum);
printf("%lld\n",mul);
printf("%f",prom);
system("Pause");
return 0;
}