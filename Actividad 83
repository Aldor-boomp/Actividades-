#include  <stdio.h>
#include  <stdlib.h>
int main (){
    int array[10]={1,2,3,4,5,6,7,8,9,0};
    int len=sizeof(array)/sizeof(int);
printf("los bytes del arrego son: %d \n",sizeof(array));
printf("cada entero tiene %d bits\n",sizeof(int));
printf("el arreglo tiene  %d elementos \n",len);
system("Pause");
return 0;
}